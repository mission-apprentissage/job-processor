{
  "name": "job-processor",
  "version": "0.0.0",
  "description": "Job processor service",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mission-apprentissage/job-processor.git"
  },
  "author": "Moroine <moroine.bentefrit@beta.gouv.fr>",
  "license": "MIT",
  "engines": {
    "node": ">=24.10",
    "npm": ">=10.0.0",
    "yarn": "please-use-npm"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "npm run lint -- --fix",
    "prettier:fix": "prettier --write -u .",
    "prettier:check": "prettier --check -u .",
    "release": "semantic-release",
    "prepare": "husky",
    "pretest": "docker compose up -d --wait",
    "test": "vitest",
    "posttest": "docker compose down -v",
    "test:ci": "npm run -- test --run",
    "build": "tsup-node --env.NODE_ENV production",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@sentry/node": "^9.29.0 || ^10.0.0",
    "@codegouvfr/react-dsfr": "^1.25.3",
    "@mui/material": "^7.1.1",
    "@mui/x-data-grid": "^8.5.2",
    "mongodb": "*",
    "react": "^19.1.0",
    "zod": "^3.25.64 || ^4.0.0",
    "zod-mongodb-schema": "^2.0.0 || ^3.0.2"
  },
  "peerDependenciesMeta": {
    "@codegouvfr/react-dsfr": {
      "optional": true
    },
    "@mui/material": {
      "optional": true
    },
    "@mui/x-data-grid": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@codegouvfr/react-dsfr": "^1.31.0",
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@eslint/compat": "^1.4.1",
    "@eslint/js": "^9.39.2",
    "@mui/material": "^7.3.7",
    "@mui/x-data-grid": "^8.26.0",
    "@semantic-release/changelog": "^6.0.3",
    "@types/react": "^19.2.9",
    "eslint": "^9.39.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-n": "^17.23.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^6.1.1",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "mongodb": "^6.21.0",
    "prettier": "^3.8.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "semantic-release": "^25.0.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1",
    "vitest": "^4.0.18",
    "zod": "^4.3.6",
    "zod-mongodb-schema": "^3.0.2"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --cache --fix",
      "prettier --write -u"
    ],
    "*": [
      "prettier --write -u"
    ]
  },
  "files": [
    "src/**/*",
    "!src/**/*.test.ts",
    "dist/**/*"
  ],
  "dependencies": {
    "cron-parser": "^5.5.0",
    "date-fns": "^4.1.0",
    "type-fest": "^5.4.1"
  }
}
