{
  "name": "job-processor",
  "version": "0.0.0",
  "description": "Job processor service",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mission-apprentissage/job-processor.git"
  },
  "author": "Moroine <moroine.bentefrit@beta.gouv.fr>",
  "license": "MIT",
  "engines": {
    "node": ">=20.3",
    "npm": ">=10.0.0",
    "yarn": "please-use-npm"
  },
  "scripts": {
    "lint": "eslint --ignore-path .gitignore --cache --ext .js,.ts,.tsx,.cjs .",
    "lint:fix": "npm run lint -- --fix",
    "prettier:fix": "prettier --write -u .",
    "prettier:check": "prettier --check -u .",
    "release": "semantic-release",
    "prepare": "husky",
    "pretest": "docker compose up -d --wait",
    "test": "vitest",
    "posttest": "docker compose down -v",
    "test:ci": "npm run -- test --run",
    "build": "tsup-node --env.NODE_ENV production",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@sentry/node": "^7.51.1 || ^8.0.0 || ^9.12.0",
    "@codegouvfr/react-dsfr": "^1.9.11",
    "@mui/material": "^5.15.18",
    "@mui/x-data-grid": "^7.5.0",
    "mongodb": "*",
    "react": "^18.0.0 || ^19.0.0",
    "zod": "^3.0.0",
    "zod-mongodb-schema": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "@codegouvfr/react-dsfr": {
      "optional": true
    },
    "@mui/material": {
      "optional": true
    },
    "@mui/x-data-grid": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@codegouvfr/react-dsfr": "^1.9.11",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.1",
    "@mui/material": "^5.17.1",
    "@mui/x-data-grid": "^7.28.2",
    "@semantic-release/changelog": "^6.0.3",
    "@types/react": "^19.1.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-import-resolver-typescript": "^3.10.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.17.0",
    "eslint-plugin-zod": "^1.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "mongodb": "^6.15.0",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "semantic-release": "^24.2.3",
    "semantic-release-slack-bot": "^4.0.2",
    "sentry-testkit": "^5.0.10",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "vitest": "^3.1.1",
    "zod": "^3.24.2",
    "zod-mongodb-schema": "^1.0.2"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --ignore-path .gitignore --cache --fix",
      "prettier --write -u"
    ],
    "*": [
      "prettier --write -u"
    ]
  },
  "files": [
    "src/**/*",
    "!src/**/*.test.ts",
    "dist/**/*"
  ],
  "dependencies": {
    "cron-parser": "^5.1.0",
    "date-fns": "^4.1.0",
    "type-fest": "^4.39.0"
  }
}
