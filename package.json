{
  "name": "job-processor",
  "version": "0.0.0",
  "description": "Job processor service",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mission-apprentissage/job-processor.git"
  },
  "author": "Moroine <moroine.bentefrit@beta.gouv.fr>",
  "license": "MIT",
  "engines": {
    "node": ">=20",
    "npm": ">=10.0.0",
    "yarn": "please-use-npm"
  },
  "scripts": {
    "lint": "eslint --ignore-path .gitignore --cache --ext .js,.ts,.cjs .",
    "lint:fix": "npm run lint -- --fix",
    "prettier:fix": "prettier --write -u .",
    "prettier:check": "prettier --check -u .",
    "release": "semantic-release",
    "prepare": "husky install",
    "test": "vitest",
    "test:ci": "npm run -- test --run",
    "build": "tsup-node --env.NODE_ENV production",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "mongodb": "*",
    "zod": "^3.0.0",
    "zod-mongodb-schema": "^1.0.0-next.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.0",
    "@commitlint/config-conventional": "^18.4.0",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.1.0",
    "@semantic-release/changelog": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-zod": "^1.4.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "mongodb": "^6.2.0",
    "prettier": "^3.0.3",
    "semantic-release": "^22.0.7",
    "semantic-release-slack-bot": "^4.0.2",
    "sentry-testkit": "^5.0.6",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6",
    "zod": "^3.22.4",
    "zod-mongodb-schema": "^1.0.0-next.3"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --ignore-path .gitignore --cache --fix",
      "prettier --write -u"
    ],
    "*": [
      "prettier --write -u"
    ]
  },
  "files": [
    "src/**/*",
    "!src/**/*.test.ts",
    "dist/**/*"
  ],
  "dependencies": {
    "@sentry/node": "^7.80.0",
    "cron-parser": "^4.9.0",
    "date-fns": "^2.30.0"
  }
}
